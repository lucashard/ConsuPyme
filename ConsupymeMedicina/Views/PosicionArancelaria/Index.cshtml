@model IEnumerable<Modelo.Posicion_Arancelaria>

@{
    ViewBag.Title = "Index";
}

<h2>Posicion Arancelaria</h2>

<p>
    @Html.ActionLink("Crear", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayName("Numero Posicion")
        </th>
        <th>
            @Html.DisplayName("Porcentaje Iva")
        </th>
        <th>
            @Html.DisplayName("Porcentaje Taza Estadistica")
        </th>
        <th>
            @Html.DisplayName("Porcentaje Importacion")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Numero_Posicion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Porcentaje_Iva)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Porcentaje_Taza_Estadistica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Porcentaje_Importacion)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
         @*   @Html.ActionLink("Details", "Details", new { id=item.Id }) |*@
            @Html.ActionLink("Borrar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
